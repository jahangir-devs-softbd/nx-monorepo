{
  "name": "@test-workspace/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "clean": "nx clear-cache",
    "format:check": "nx format:check --all",
    "format:write": "nx format:write --all",
    "build": "nx run-many --target=build --output-style=stream",
    "setup:dev": "ts-node ./scripts/setup.ts",
    "docker:dev": "docker-compose -f docker-compose.dev.yml --env-file .env.docker-compose up",
    "docker:dev:cleanup": "docker-compose -f docker-compose.dev.yml --env-file .env.docker-compose down",
    "serve:test-application": "nx serve test-application",
    "serve:second-app": "nx serve second-app",
    "prepare": "husky",
    "precommit": "nx lint-staged",
    "db:prisma:format-schema": "prisma format",
    "db:prisma:generate-typings": "prisma generate",
    "db:prisma:gui": "prisma studio",
    "db:prisma:gui:prod": "npx dotenv-cli -e .env.prod -- prisma studio",
    "db:prisma:migrate": "prisma migrate deploy",
    "db:prisma:push": "prisma db push",
    "db:prisma:seed": "prisma db seed",
    "db:prisma:setup": "yarn db:prisma:push && yarn db:prisma:seed",
    "db:prisma:validate-schema": "prisma validate",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:typeorm:drop": "env-cmd -f .env yarn typeorm -- schema:drop -d libs/shared/src/config/datasource.ts",
    "db:typeorm:migration:create": "env-cmd -f .env yarn typeorm -- migration:create",
    "db.typeorm:migration:run": "env-cmd -f .env yarn typeorm migration:run -d libs/shared/src/config/datasource.ts",
    "db.typeorm:migration:rollback": "env-cmd -f .env yarn typeorm -- migration:revert -d libs/shared/src/config/datasource.ts",
    "db.typeorm:seed:run": "env-cmd -f .env ts-node -r tsconfig-paths/register apps/seeder/src/main.ts seeder",
    "dep-graph": "nx dep-graph",
    "e2e": "ng e2e",
    "help": "nx help",
    "lint": "nx lint",
    "lint:all": "nx run-many --target=lint --all",
    "lint:varbose": "nx lint --verbose"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "axios": "^1.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "19.2.2",
    "@nx/eslint-plugin": "19.2.2",
    "@nx/jest": "19.2.2",
    "@nx/js": "19.2.2",
    "@nx/nest": "19.2.2",
    "@nx/node": "19.2.2",
    "@nx/web": "19.2.2",
    "@nx/webpack": "19.2.2",
    "@nx/workspace": "19.2.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.16.9",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^9.0.11",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "lint-staged": "^15.2.5",
    "ng": "^0.0.0",
    "nx": "19.2.2",
    "prettier": "^2.6.2",
    "semver": "^7.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.2",
    "webpack-cli": "^5.1.4",
    "winston": "^3.13.0"
  },
  "engines": {
    "node": "^20.11.0",
    "npm": "^10.2.4"
  }
}
